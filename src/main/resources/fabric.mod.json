{
  "schemaVersion": 1,
  "id": "create-controllable-bridge",
  "version": "${version}",
  "name": "Create Controllable Bridge",
  "description": "A simple Fabric mod to bridge between Create and Controllable to enable gamepad controls for specific Create entities.",
  "authors": [
    "petermeansrock"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/create-fabric-addon-template"
  },
  "license": "CC0",
  "icon": "assets/create-controllable-bridge/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.petermeansrock.minecraft.create.controllable.CreateControllableBridgeMod"
    ]
  },
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
